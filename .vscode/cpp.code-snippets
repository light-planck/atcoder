				"bool IsPrime(long long n) {",
				"  if (n <= 1) return false;",
				"",
				"  for (long long i = 2; i * i <= n; ++i) {",
				"    if (n % i == 0) return false;",
				"  }",
				"",
				"  return true;",
				"}",
		],
		"description": ""
	},

	"factorprime": {
		"prefix": "factorprime",
		"body": [
				"// 素因数分解",
				"vector<pair<ll, ll>> FactorPrime(ll n) {",
				"  vector<pair<ll, ll>> prime;",
				"",
				"  for (ll i = 2; i * i <= n; ++i) {",
				"    if (n % i != 0) continue;",
				"",
				"    ll exponent = 0;",
				"    while (n % i == 0) {",
				"      ++exponent;",
				"      n /= i;",
				"    }",
				"",
				"    prime.push_back(make_pair(i, exponent));",
				"  }",
				"",
				"  // 上の処理後にnが1でないならnは素数",
				"  if (n != 1) {",
				"    prime.push_back(make_pair(n, 1));",
				"  }",
				"",
				"  return prime;",
				"}",
		],
		"description": ""
	},

	"comb": {
		"prefix": "comb",
		"body": [
				"ll comb(int n, int r) {",
				"  ll ans = 1;",
				"  for (int i = 0; i < r; i++) {",
				"    ans *= n - i;",
				"    ans /= i + 1;",
				"  }",
				"  return ans;",
				"}",
		],
		"description": ""
	},

